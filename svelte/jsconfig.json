{
	"compilerOptions": {
		"moduleResolution": "bundler",
		"target": "ESNext",
		"module": "ESNext",
		"verbatimModuleSyntax": true,
		"isolatedModules": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"checkJs": true,
		"paths": {
			"svelte-icons-pack": ["./node_modules/svelte-icons-pack/dist"]}
	},
	"include": ["./**/*.js", "./**/*.svelte"]
}